    // Part 4
#include <iostream>
using namespace std;

int main() {
   int numSeconds;

   // Our tests will run your program with input 2, then run again with input 5.
   // Your program should work for any input, though.
   cin >> numSeconds;

   cout << numSeconds << endl;

   return 0;
}


    // Part 5
